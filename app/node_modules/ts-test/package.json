{
  "name": "ts-test",
  "version": "0.0.3",
  "description": "A collection of test utilites",
  "main": "index.ts",
  "bin": {
    "jasmine-ts": "bin/jasmine-ts.js"
  },
  "scripts": {
    "install": "typings i",
    "prepublish": "npm run check && tsc bin/jasmine-ts.ts && chmod +x bin/jasmine-ts.js && npm run test",
    "check": "tslint 'src/**/*.ts'",
    "test": "bin/jasmine-ts.js 'src/**/*.spec.ts'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/svi3c/node-test-utils.git"
  },
  "keywords": [
    "node",
    "testing",
    "tests",
    "jasmine"
  ],
  "author": "Sven Reglitzki <sreglitzki@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/svi3c/node-test-utils/issues"
  },
  "homepage": "https://github.com/svi3c/node-test-utils#readme",
  "dependencies": {
    "asyncawait": "^1.0.3",
    "clarify": "^1.0.5",
    "jasmine": "^2.4.1",
    "request": "^2.72.0",
    "ts-node": "^0.7.2",
    "typescript": "^1.8.10"
  },
  "devDependencies": {
    "body-parser": "^1.15.0",
    "express": "^4.13.4",
    "http-status-enum": "^1.0.2",
    "mime": "^1.3.4",
    "tslint": "^3.7.4",
    "typings": "^0.8.1"
  }
}
